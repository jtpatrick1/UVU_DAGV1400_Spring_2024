
using System;

public class ClassExample
{
	public ClassWithFunctions TestingThings;
	
    public void Main()
	{
		TestingThings = new ClassWithFunctions();
		TestingThings.OtherFunction();
	}
}

public class ClassWithVariables
{
    public int number = 2;
    public string userName = "Bob";
	public float strangeNumber = 1.2f;
	public string playerTwo = "Richard";
}

public class ClassWithFunctions
{
    public void RunFunction()
    {
        Console.WriteLine("Running Function");
    }
	public void OtherFunction()
	{
		Console.WriteLine("Hello There");
	}
}  